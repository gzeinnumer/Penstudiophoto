<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="id"
        android:textStyle="bold"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/tv_idpaketphoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="id paket photo"
        android:textStyle="bold"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/tv_idjenispaket"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="id jenis paket"
        android:textStyle="bold"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/tv_namapaketphoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama Paket Photo"
        android:textStyle="bold"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/tv_tglupdate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tanggal Update"
        android:textStyle="bold"
        android:textSize="20dp"/>


</LinearLayout>